{
  "dotfiles": {
    "_vimrc": {
      "type": "file",
      "source": "./.files/.vimrc",
      "destination": "$HOME/.vimrc"
    },
    ".tmux.conf": {
      "type": "file",
      "source": "./.files/.tmux.conf",
      "destination": "$HOME/.tmux.conf"
    },
    ".bashfuncs": {
      "type": "directory",
      "source": "./.bashfuncs",
      "destination": "$HOME/.bashfuncs"
    },
    "bash_profile": {
      "type": "file",
      "source": "./.files/.bash_profile",
      "destination": "$HOME/.bash_profile"
    },
    "sway_conf": {
      "type": "file",
      "source": "./.files/config",
      "destination": "$HOME/.config/sway/config"
    },
    "foot_conf": {
      "type": "file",
      "source": "./.files/foot.ini",
      "destination": "$HOME/.config/foot/foot.ini"
    },
    "vifm_conf": {
      "type": "file",
      "source": "./.files/vifmrc",
      "destination": "$HOME/.vifm/vifmrc"
    },
    "waybar_config": {
      "type": "file",
      "source": "./.files/waybar_cfg",
      "destination": "$HOME/.config/waybar/config"
    }
  }
}